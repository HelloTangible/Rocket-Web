{
  "name": "rocket-web",
  "version": "0.0.1",
  "description": "Website for Project Rocket",
  "repository": "https://github.com/HelloTangible/rocket-web",
  "license": "MIT",
  "scripts": {
    "start": "next start",
    "dev": "next",
    "build": "next build"
  },
  "devDependencies": {
    "standard": "^9.0.2"
  },
  "dependencies": {
    "auth0-js": "^8.5.0",
    "axios": "^0.15.3",
    "dotenv": "^4.0.0",
    "jwt-decode": "^2.2.0",
    "next": "^2.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^4.0.0"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "string-quotes": "single",
      "property-no-unknown": [
        true,
        {
          "ignoreProperties": [
            "composes"
          ]
        }
      ],
      "globals": [
        "window",
        "location",
        "localStorage"
      ],
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global",
            "local"
          ]
        }
      ]
    }
  }
}
